{
    "name": "davidianbonner/cli-phone-validator",
    "description": "CLI Phone Number Validator using Google's libphonenumber.",
    "keywords": ["framework", "laravel", "console", "zero"],
    "license": "MIT",
    "authors": [
        {
            "name": "David Bonner",
            "email": "davidianbonner@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "giggsey/libphonenumber-for-php": "^8.8",
        "laravel-zero/framework": "4.0.*",
        "vlucas/phpdotenv": "^2.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.4",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["number-validator"]
}
